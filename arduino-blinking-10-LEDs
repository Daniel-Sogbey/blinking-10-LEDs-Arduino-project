void setup() {
  // put your setup code here, to run once:
  pinMode(13,OUTPUT);//red
  pinMode(6,OUTPUT);//yellow
  pinMode(8,OUTPUT);//green

}



void loop() {
  // put your main code here, to run repeatedly:
  digitalWrite(13,HIGH);
  delay(50);
  digitalWrite(8,HIGH);
  delay(100);
  digitalWrite(13,LOW);
  delay(10);  
    digitalWrite(8,LOW);
  delay(50);
  

}
